n","    classval[i] = 0\n","  else:\n","    classval[i] = 1\n","\n","classval = classval.reshape([200, 200])\n","fig, ax = plt.subplots()\n","plt.contourf(x1line, x2line, classval)\n","plt.scatter(X[y==1,0], X[y==1,1])\n","plt.scatter(X[y==0,0], X[y==0,1])"],"execution_count":30,"outputs":[{"output_type":"execute_result","data":{"text/plain":["<matplotlib.collections.PathCollection at 0x7fe52641aac8>"]},"metadata":{"tags":[]},"execution_count":30},{"output_type":"display_data","data":{"image/png":"\n","text/plain":["<Figure size 432x288 with 1 Axes>"]},"metadata":{"tags":[],"needs_background":"light"}}]},{"cell_type":"code","metadata":{"colab":{"base_uri":"https://localhost:8080/","height":287},"id":"NFI-3U63QTFu","executionInfo":{"status":"ok","timestamp":1605970437906,"user_tz":0,"elapsed":2117,"user":{"displayName":"Asad Raja","photoUrl":"https://lh3.googleusercontent.com/-qBjKzlIWRd8/AAAAAAAAAAI/AAAAAAAAEkA/VuUBV3qSGTk/s64/photo.jpg","userId":"01745811664429108254"}},"outputId":"7ef67a9a-33ab-4a62-9747-c0bc30eab9f5"},"source":["from sklearn.neighbors import KNeighborsClassifier\n","near = 10\n","neigh = KNeighborsClassifier(n_neighbors=near) \n","neigh.fit(X, y)\n","A = neigh.predict(Xgrid).reshape([200, 200])\n","fig, ax = plt.subplots()\n","plt.contourf(x1line, x2line, A)\n","plt.scatter(X[y==1,0], X[y==1,1])\n","plt.scatter(X[y==0,0], X[y==0,1])"],"execution_count":43,"outputs":[{"output_type":"execute_result","data":{"text/plain":["<matplotlib.collections.PathCollection at 0x7fe525f5e5c0>"]},"metadata":{"tags":[]},"execution_count":43},{"output_type":"display_data","data":{"image/png":"\n","text/plain":["<Figure size 432x288 with 1 Axes>"]},"metadata":{"tags":[],"needs_background":"light"}}]}]}